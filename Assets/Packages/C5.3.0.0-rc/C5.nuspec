    <tags>Generics Collections Heap TreeSet PriorityQueue</tags>
    <repository type="git" url="https://github.com/sestoft/C5" />
    <dependencies>
      <group targetFramework="net6.0" />
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
  </metadata>
</package>